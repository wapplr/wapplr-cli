{
    "parser": "babel-eslint",
    "extends": [
        "eslint-config-wapplr"
    ],
    "env": {
        "browser": true,
        "commonjs": true,
        "node": true,
        "es6": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
        "no-console": "off",
        "strict": ["error", "global"],
        "curly": "warn"
    }
}
